{
	"name": "obsidian-zettlelflow",
	"version": "0.0.1",
	"description": "A plugin for Obsidian that allows you to create and manage notes in a Zettlekasten-like way.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest --colors --coverage test",
		"sonar": "npx eslint --ignore-path .gitignore src/**/*.ts",
		"pretest": "eslint --ignore-path .gitignore src/**/*.ts"
	},
	"keywords": [],
	"author": "RafaelGB",
	"license": "MIT",
	"devDependencies": {
		"@types/cron": "^2.4.0",
		"@types/node": "20.5.1",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"@typescript-eslint/eslint-plugin": "6.4.0",
		"@typescript-eslint/parser": "6.4.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.19.2",
		"eslint": "8.47.0",
		"jest": "29.6.3",
		"obsidian": "1.4.0",
		"ts-jest": "29.1.1",
		"tslib": "2.6.2",
		"typescript": "5.1.6",
		"zustand": "4.4.1"
	},
	"dependencies": {
		"@popperjs/core": "2.11.8",
		"cron": "2.4.1",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	}
}
