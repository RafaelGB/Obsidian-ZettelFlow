  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/modifiers": "9.0.0",
    "@popperjs/core": "2.11.8",
    "json-stable-stringify": "1.3.0",
    "monkey-around": "3.0.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tiny-jsonc": "1.0.2",
    "use-sync-external-store": "1.5.0",
    "uuid": "11.1.0"
