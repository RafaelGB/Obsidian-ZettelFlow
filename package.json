    "eslint": "8.50.0",
    "eslint-plugin-react": "7.33.2",
    "jest": "29.7.0",
    "obsidian": "1.4.11",
    "ts-jest": "29.1.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "zustand": "4.4.1"
